<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter"><meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
     <head><title>${message:page-title}</title></head>
   
	<t:border><div id="userlistDiv">
	
		<h1>${message:create-profile}</h1><hr/>
		<infouser>
			
				<t:beaneditform t:id="form" submitlabel="${message:send-but}" object="profile" add="repeat" reorder="admin,email,password,repeat" exclude="owner">
					
					<p:email>
						<t:label for="email"/>
						<t:textfield t:id="email" value="profile.email" />
						<t:if test="isnotAvalible">
							${message:non-available-email}
						</t:if>
					</p:email>
					<p:password>
						<t:label for="password"/>
						<t:passwordfield t:id="password" value="password" validate="required,minLength=6"/>
						<t:if test="isnotPassOk">
							${message:pass-not-match}
						</t:if>
					</p:password>
					<p:repeat>
						<t:label for="repeat"/>
						<t:passwordfield t:id="repeat" value="repeat" validate="required,minLength=6"/>
					</p:repeat>
				</t:beaneditform>
				
			</t:if>
			
			
			
			
		</infouser>
	</div>
	</t:border>
	
		
			
	


</html>
