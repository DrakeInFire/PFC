<html xmlns:p="tapestry:parameter"><meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
   
	<t:border xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
			
		<t:zone t:id="logedZone" id="logedZone">
			<h1>Usuarios Logeados</h1>
			<t:grid source="usersLoged" row="userLoged" add="kick" rowsperpage="15" inplace="true">
				<p:kickcell>
					<t:actionlink t:id="kickbut" context="userLoged.idSession" zone="^"><button>kick</button></t:actionlink>
				</p:kickcell>
			</t:grid>
		</t:zone>
		<t:zone t:id="logZone" id="logZone">
			<h1>Log de usuarios</h1>
			<t:if test="main">
				<t:form>
					<label>ShowAll</label><input t:type="checkbox" t:id="showAll" value="showAll" onclick="this.form.submit()"/>
				</t:form>
				<t:grid source="logs" row="log" rowsperpage="15" inplace="true" />
				
				<p:else>
					<t:grid source="logs" row="log" rowsperpage="15" inplace="true" exclude="accessTo" />
				</p:else>
			</t:if>
		</t:zone>
			
			
			
			
	</t:border>
	
		
			
	


</html>
